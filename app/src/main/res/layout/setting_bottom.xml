<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">


            <Button
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_marginStart="30dp"
                android:layout_marginEnd="30dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/round_border"
                android:text="More Options..."
                android:textAllCaps="false"
                android:elevation="5dp"
                android:visibility="gone"
                android:textColor="@color/colorPrimaryDark"
                android:fontFamily="@font/cabin"/>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:layout_marginTop="15dp"
                android:background="@color/explore_color"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/swipe_text"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:src="@drawable/ic_reset_swipes"
                    android:tint="@color/gold"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="10dp"/>
                <ImageView
                    android:id="@+id/activate_swipe"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginEnd="10dp"
                    android:src="@drawable/ic_baseline_chevron_right_24"
                    />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_toEndOf="@+id/swipe_text"
                    android:layout_toStartOf="@+id/activate_swipe"
                    android:gravity="center_vertical"
                    android:layout_marginStart="15dp"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Marital Status"
                        android:textSize="18dp"
                        android:fontFamily="@font/cabin"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="All"
                        android:fontFamily="@font/cabin"
                        android:layout_marginTop="10dp"/>
                </LinearLayout>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:layout_marginTop="15dp"
                android:background="@color/explore_color"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/change_image"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:src="@drawable/ic_work"
                    android:tint="@color/gold"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="10dp"/>
                <ImageView

                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginEnd="10dp"
                    android:src="@drawable/ic_baseline_chevron_right_24"
                    />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_toEndOf="@+id/change_image"
                    android:layout_toStartOf="@+id/activate_change"
                    android:gravity="center_vertical"
                    android:layout_marginStart="15dp"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Profession"
                        android:textSize="18dp"
                        android:fontFamily="@font/cabin"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="All"
                        android:fontFamily="@font/cabin"
                        android:layout_marginTop="10dp"/>
                </LinearLayout>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:layout_marginTop="15dp"
                android:background="@color/explore_color"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/chat_image"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:src="@drawable/ic_pref"
                    android:tint="@color/gold"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="10dp"/>
                <ImageView
                    android:id="@+id/activate_chat"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginEnd="10dp"
                    android:src="@drawable/ic_baseline_chevron_right_24"
                    />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_toEndOf="@+id/chat_image"
                    android:layout_toStartOf="@+id/activate_chat"
                    android:gravity="center_vertical"
                    android:layout_marginStart="15dp"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Education"
                        android:textSize="18dp"
                        android:fontFamily="@font/cabin"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="All"
                        android:fontFamily="@font/cabin"
                        android:layout_marginTop="10dp"/>
                </LinearLayout>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:layout_marginTop="15dp"
                android:background="@color/explore_color"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/reset_image"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:src="@drawable/ic_flight"
                    android:tint="@color/gold"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="10dp"/>
                <com.suke.widget.SwitchButton
                    android:id="@+id/activate_reset"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginEnd="10dp"
                    />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_toEndOf="@+id/reset_image"
                    android:layout_toStartOf="@+id/activate_reset"
                    android:gravity="center_vertical"
                    android:layout_marginStart="15dp"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="If abroad, willing to relocate"
                        android:textSize="18dp"
                        android:fontFamily="@font/cabin"/>

                </LinearLayout>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:layout_marginTop="15dp"
                android:background="@color/explore_color"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/profile_boost"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:src="@drawable/ic_baby"
                    android:tint="@color/gold"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="10dp"/>
                <com.suke.widget.SwitchButton
                    android:id="@+id/activate_boost"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginEnd="10dp"
                    />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_toEndOf="@+id/profile_boost"
                    android:layout_toStartOf="@+id/activate_boost"
                    android:gravity="center_vertical"
                    android:layout_marginStart="15dp"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Without children"
                        android:textSize="18dp"
                        android:fontFamily="@font/cabin"/>
                </LinearLayout>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:layout_marginTop="15dp"
                android:background="@color/explore_color"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/private_filter"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:src="@drawable/ic_height"
                    android:tint="@color/gold"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="10dp"/>
                <ImageView
                    android:id="@+id/activate_private_filter"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginEnd="10dp"
                    android:src="@drawable/ic_baseline_chevron_right_24"
                    />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_toEndOf="@+id/private_filter"
                    android:layout_toStartOf="@+id/activate_private_filter"
                    android:gravity="center_vertical"
                    android:layout_marginStart="15dp"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Height"
                        android:textSize="18dp"
                        android:fontFamily="@font/cabin"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Any Height"
                        android:fontFamily="@font/cabin"
                        android:layout_marginTop="10dp"/>
                </LinearLayout>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:layout_marginTop="15dp"
                android:background="@color/explore_color"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/pref_image"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:src="@drawable/ic_calendar"
                    android:tint="@color/gold"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="10dp"/>
                <ImageView
                    android:id="@+id/activate_pref"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginEnd="10dp"
                    android:src="@drawable/ic_baseline_chevron_right_24"
                    />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_toEndOf="@+id/pref_image"
                    android:layout_toStartOf="@+id/activate_pref"
                    android:gravity="center_vertical"
                    android:layout_marginStart="15dp"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Marriage Plans"
                        android:textSize="18dp"
                        android:fontFamily="@font/cabin"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Any timeframe"
                        android:fontFamily="@font/cabin"
                        android:layout_marginTop="10dp"/>
                </LinearLayout>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:layout_marginTop="15dp"
                android:background="@color/explore_color"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/badge_image"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:src="@drawable/ic_pray"
                    android:tint="@color/gold"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="10dp"/>
                <ImageView
                    android:id="@+id/activate_badge"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginEnd="10dp"
                    android:src="@drawable/ic_baseline_chevron_right_24"
                    />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_toEndOf="@+id/badge_image"
                    android:layout_toStartOf="@+id/activate_badge"
                    android:gravity="center_vertical"
                    android:visibility="gone"
                    android:layout_marginStart="15dp"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Prayer"
                        android:textSize="18dp"
                        android:fontFamily="@font/cabin"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="All prayer levels"
                        android:fontFamily="@font/cabin"
                        android:layout_marginTop="10dp"/>
                </LinearLayout>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:layout_marginTop="15dp"
                android:background="@color/explore_color"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/ads_image"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:src="@drawable/ic_religion"
                    android:tint="@color/gold"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="10dp"/>
                <ImageView
                    android:id="@+id/activate_no_ads"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginEnd="10dp"
                    android:src="@drawable/ic_baseline_chevron_right_24"
                    />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_toEndOf="@+id/ads_image"
                    android:layout_toStartOf="@+id/activate_no_ads"
                    android:gravity="center_vertical"
                    android:layout_marginStart="15dp"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Religiosity"
                        android:textSize="18dp"
                        android:fontFamily="@font/cabin"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="All Religiosity"
                        android:fontFamily="@font/cabin"
                        android:layout_marginTop="10dp"/>
                </LinearLayout>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:layout_marginTop="15dp"
                android:background="@color/explore_color"
                android:visibility="gone"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/support_image"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:src="@drawable/ic_supporter"
                    android:tint="@color/gold"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="10dp"/>
                <com.suke.widget.SwitchButton
                    android:id="@+id/activate_supporter"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginEnd="10dp"
                    />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_toEndOf="@+id/support_image"
                    android:layout_toStartOf="@+id/activate_supporter"
                    android:gravity="center_vertical"
                    android:layout_marginStart="15dp"
                    android:visibility="gone"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Supporter"
                        android:textSize="18dp"
                        android:fontFamily="@font/cabin"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Help us transform how Senegalese marry. Learn more"
                        android:fontFamily="@font/cabin"
                        android:layout_marginTop="10dp"/>
                </LinearLayout>
            </RelativeLayout>
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:src="@drawable/logonew"/>
            <LinearLayout
                android:id="@+id/free_trail_one"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_marginStart="30dp"
                android:layout_marginEnd="30dp"
                android:layout_marginTop="20dp"
                android:gravity="center"
                android:elevation="5dp"
                android:layout_marginBottom="30dp"
                android:orientation="vertical"
                android:background="@drawable/round_corner">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:text="Start 7 day FREE trial"
                    android:textStyle="bold"
                    android:visibility="gone"
                    android:textColor="@color/white"
                    android:fontFamily="@font/cabin"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:text="Upgrade to unlock"
                    android:textColor="@color/white"
                    android:fontFamily="@font/cabin"/>

            </LinearLayout>

            <Button
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_marginStart="30dp"
                android:layout_marginEnd="30dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/round_border"
                android:text="More Options..."
                android:textAllCaps="false"
                android:elevation="5dp"
                android:visibility="gone"
                android:textColor="@color/colorPrimaryDark"
                android:fontFamily="@font/cabin"/>
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="400dp"
                android:visibility="gone"
                android:src="@drawable/setting_bottom_imagr"/>
            <LinearLayout
                android:id="@+id/free_trail_two"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_marginStart="30dp"
                android:layout_marginEnd="30dp"
                android:layout_marginTop="20dp"
                android:gravity="center"
                android:elevation="5dp"
                android:visibility="gone"
                android:orientation="vertical"
                android:background="@drawable/round_corner">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:text="Start 7 day FREE trial"
                    android:textStyle="bold"
                    android:textColor="@color/white"
                    android:fontFamily="@font/cabin"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:text="Then Rs. 699.00 per week"
                    android:textColor="@color/white"
                    android:fontFamily="@font/cabin"/>

            </LinearLayout>
            <Button
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_marginStart="30dp"
                android:layout_marginEnd="30dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/round_border"
                android:text="More Options..."
                android:textAllCaps="false"
                android:elevation="5dp"
                android:visibility="gone"
                android:textColor="@color/colorPrimaryDark"
                android:fontFamily="@font/cabin"/>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
